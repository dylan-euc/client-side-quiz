{
  "name": "client-side-quiz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "docker-compose exec -T postgres psql -U quiz -d quiz -f /docker-entrypoint-initdb.d/init.sql",
